    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch CLI (Interactive REPL)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "BAIDUPAN_CLI_TOKEN_DIR": "${workspaceFolder}/.debug"
            },
            "args": [
                "-c",
                "${workspaceFolder}/config-local.yaml",
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "Launch rename (direct)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "BAIDUPAN_CLI_TOKEN_DIR": "${workspaceFolder}/.debug"
            },
            "args": [
                "-c",
                "${workspaceFolder}/config-local.yaml",
                "-t",
                "--access-token",
                "",
                "rename",
                "--path",
                "/a/b.txt",
                "--newname",
                "c.txt"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "Test: cmd (rename tests)",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/cmd",
            "cwd": "${workspaceFolder}",
            "args": [
                "-test.run",
                "TestBuildRenameFilelist"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}